{
  "name": "open-cobol",
  "version": "0.1.0",
  "description": "A node wrapper method to pass COBOL code to open-cobol (Linux)",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "tbd": "ava ./tests/manual.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joel-chu/node-open-cobol.git"
  },
  "keywords": [
    "COBOL",
    "node",
    "node.js",
    "open-cobol",
    "linux",
    "docker"
  ],
  "author": "Joel Chu <joelchu@foxmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joel-chu/node-open-cobol/issues"
  },
  "homepage": "https://github.com/joel-chu/node-open-cobol#readme",
  "devDependencies": {
    "ava": "^3.13.0"
  },
  "dependencies": {
    "tmp": "^0.2.1",
    "which": "^2.0.2",
    "yargs": "^16.1.0"
  },
  "ava": {
    "files": [
      "tests/**/*.test.js"
    ],
    "ignoredByWatcher": [
      "tests/fixtures/**/*",
      "lib/**/*"
    ],
    "verbose": true,
    "tap": false,
    "failFast": true
  },
  "bin": {
    "nodecobc": "./cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
